---
import Slide from "../../../components/Slide.astro";
import { ImageDemo } from "./demo";
---

<Slide>
	<ImageDemo client:visible />
</Slide>

<Slide>
	<img
		src="/images/media-attribute/link.webp"
		alt="media attribute on a link element"
	/>
</Slide>

<Slide>
	<img
		src="/images/media-attribute/print.webp"
		alt="media attribute on a link element for print"
	/>
</Slide>

<Slide>
	<img
		src="/images/media-attribute/video.webp"
		alt="media attribute on a video element"
	/>
</Slide>

<Slide>
	<img
		src="/images/media-attribute/audio.webp"
		alt="media attribute on an audio element"
	/>
</Slide>
